{
    "name": "bsmrg-gmbh/arcanadev-log-viewer",
    "description": "Provides a Log Viewer for Laravel",
    "keywords": [
        "arcanedev",
        "arcanesoft",
        "laravel",
        "log",
        "log viewer",
        "log-viewer",
        "logviewer"
    ],
    "homepage": "https://github.com/ARCANEDEV/LogViewer",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/BMSRG-GmbH/ARCANADEV-Support"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "bsmrg-gmbh/arcanadev-support": "^12.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0|^12.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench-core": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LogViewer\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LogViewer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:dox": "phpunit --testdox --colors=always",
        "test:cov": "phpunit --coverage-html build/coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\LogViewer\\LogViewerServiceProvider",
                "Arcanedev\\LogViewer\\Providers\\DeferredServicesProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
